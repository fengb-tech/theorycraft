{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "no-unused-vars": [2, {"args": "after-used", "argsIgnorePattern": "^_"}],
    "no-alert": 2,
    "no-eval": 2,
    "no-implied-eval": 2,
    "no-trailing-spaces": 2,
    "brace-style": 2,
    "curly": 2,
    "eqeqeq": [2, "smart"],
    "semi": [2, "never"]
  }
}
